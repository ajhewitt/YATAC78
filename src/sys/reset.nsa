# reset vector
INCLUDE ../inc/unary.nsa
INCLUDE ../inc/zpage.nsa
INCLUDE ../inc/pages.nsa
PAGE RESET

ADDR 0X80
LD HL, $0x31        # VGA, 8x8 text
LD Y, $VMODE
MVHLZ ND            # store video mode

# ROM Copy
LD HL, $0
LD Y, $VSTART
MULH DZ, ND             # init vstart to 0
LD Y, $ROM_ID
MULH DZ, ND             # init rom to 0
LD Y, $ROM_Y
MULH DZ, ND             # init v-porch to 0
LD HL, $0xC
LD Y, $ROM_N
MVHLZ ND                # init 12 - copy 13 roms

# Start Video
LD PG, $ROM_COPY
